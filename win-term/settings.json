    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+\\"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "closePane",
            "keys": "ctrl+w"
        }
    ],
